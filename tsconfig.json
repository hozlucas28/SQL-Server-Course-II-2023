{
	"compilerOptions": {
		"target": "es2016",
		"lib": ["es6"],
		"module": "ES2022",
		"rootDir": "practical-work",
		"allowJs": true,
		"outDir": "practical-work/dist",
		"removeComments": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
		"moduleResolution": "Node"
	},
	"include": ["practical-work/**/*"]
}
